CREATE TABLE student (
	rollno INT PRIMARY KEY,
    name VARCHAR(50)
);

SELECT * FROM student;

INSERT INTO student 
( rollno, name)
VALUES
(101, "Shahbaz"),
(102, "Aamir"),
(103, "Harsh");

INSERT INTO student 
( rollno, name)
VALUES
(104, "Siddhant");


// Creating another database of employees.

CREATE DATABASE xyz_company;
USE xyz_company;

CREATE TABLE employee(
	id INT PRIMARY KEY,
    name VARCHAR(100),
    salary INT
);

INSERT INTO employee
( id, name, salary)
VALUES
(1, "Holly", 25000),
(2, "Shahbaz", 800000),
(3, "Aman", 10000);

SELECT * FROM employee;
